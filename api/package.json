{
  "scripts": {
    "start": "NODE_ENV=prod node -r dotenv/config dist/bin/server.js",
    "start-pm2": "NODE_ENV=prod pm2 start dist/bin/server.js",
    "build": "babel src --extensions \".js,.ts,.tsx\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "NODE_ENV=dev tsnd -r tsconfig-paths/register -r dotenv/config --respawn --transpile-only --ignore-watch node_modules --no-notify src/bin/server.ts",
    "test": "NODE_ENV=test jest -i --forceExit",
    "testsh": "bash ./scripts/test.sh"
  },
  "dependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-typescript": "7.13.0",
    "@types/node": "14.14.41",
    "babel-plugin-module-resolver": "4.1.0",
    "body-parser": "1.19.0",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/supertest": "2.0.11",
    "jest": "26.6.3",
    "pm2": "4.5.6",
    "supertest": "6.1.3",
    "ts-jest": "26.5.5",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4"
  }
}